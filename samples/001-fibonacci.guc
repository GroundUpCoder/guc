const fib = n => if n < 2 then 1 else fib(n - 1) + fib(n - 2)
fib(0)
fib(1)
fib(2)
fib(3)
fib(4)
fib(5)
fib(6)
fib(7)
fib(8)
fib(9)
fib(10)
fib(11)

// exFib is a function
const exFib = (n, a, b) => do {
  (if n < 2 then 1 else a * exFib(n - 1, a, b) + b * exFib(n - 2, a, b))
}
exFib(0, 3, 2)
exFib(1, 3, 2)
exFib(2, 3, 2)
exFib(3, 3, 2)
exFib(4, 3, 2)
exFib(5, 3, 2)
exFib(6, 3, 2)
exFib(7, 3, 2)
exFib(8, 3, 2)
exFib(9, 3, 2)
exFib(10, 3, 2)
exFib(11, 3, 2)
exFib(12, 3, 2)
exFib(13, 3, 2)
